{
    "name": "Mediumify",
    "version": "1.1",
    "description": "Extend your Medium experience",
    "manifest_version": 2,
    
    
    "content_scripts": [
        {
            "matches": ["https://*.medium.com/*"],
            "js": ["contentScript.js"]
        }
    ],
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" 
    }
}